<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.security.springsecurity.dao.UserErrorLogDAO">

	<insert id="registerUserErrorLogInfo">
		<![CDATA[
			INSERT INTO USER_ERROR_LOG(
					USER_NAME 			
					,USER_TYPE 			
					,REGISTER_DATE 		
					,REGISTER_TIME 		
					,URL					
					,IP_ADDRESS 			 
					,DEVICE_NAME 		
					,ERROR_CODE 			 
					,ERROR_DESCRIPTION 	 
					,ERROR_CAUSE 		
			)
			VALUES (
					 #{userName} 			
					,#{userType} 			
					,#{registerDate} 		
					,#{registerTime} 		
					,coalesce( #{url}, '' )				
					,coalesce( #{ipAddress}, '' )	 			 
					,coalesce( #{deviceName}, '' )	 		
					,coalesce( #{errorCode}, '' )	 			 
					,coalesce( #{errorDescription}, '' )	 	 
					,coalesce( #{errorCause}, '' )	 	
			)
		]]>
	</insert>
	
</mapper>